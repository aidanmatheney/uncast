{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Database": {
      "LogLevel": "Information",
      "FlushInterval": "00:00:30"
    }
  },
  "IdentityServer": {
    "Clients": {
      "uncast-webapp": {
        "Profile": "SPA",
        "RedirectUri": "http://localhost:3000/authentication/sign-in-callback",
        "LogoutUri": "http://localhost:3000/authentication/sign-out-callback"
      }
    }
  },
  "AllowedHosts": "*"
}